{
  "version": 2,
  "builds": [
    {
      "src": "scanner/api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "includeFiles": "scanner/**"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "scanner/api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "scanner:."
  }
}